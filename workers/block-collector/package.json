  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "typescript": "^5.3.0",
    "wrangler": "^3.90.0"
  },
  "dependencies": {
    "@cloudflare/puppeteer": "^1.0.4",
    "linkedom": "^0.18.0"
  }
}
